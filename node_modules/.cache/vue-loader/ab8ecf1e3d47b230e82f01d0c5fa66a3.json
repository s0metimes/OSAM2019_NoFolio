rollTop;\n\n                var shareboardAnimation = setInterval(function() {\n                    if(shareboard.scrollTop <= 0) {\n                        shareboard.scrollTop = 0;\n                        clearInterval(shareboardAnimation);\n                        return;\n                    }\n                    else {\n                        shareboard.scrollTop -= (scrollTopValue / 10);\n                    }\n                }, 25);\n\n                this.data();\n\n            }\n        }\n    }\n\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n    p{\n        padding:0;\n        margin : 0;\n    }\n    .shareboard{\n        display:flex;\n        padding: 8px 120px;\n        width:100%;\n        height:calc(100vh - 128px);\n        background-color:rgba(52,152,219,0.2);\n        flex-flow:row wrap;\n        justify-content:space-around;\n        align-content:flex-start;\n        overflow: auto;\n    }\n    .box{\n        display:flex;\n        flex-flow:column;\n        position: relative;\n        height: 36vw;\n        width: 24vw;\n\n        min-height: 360px;\n        min-width: 240px;\n        max-height: 480px;\n        max-width: 360px;\n        margin:12px;\n        background-color:white;\n        box-shadow:0 2px 12px rgba(0,0,0,0.12);\n        padding:16px;\n    }\n    .name {\n        padding:0 16px;\n        flex:1;\n        font-size: 16px;\n        font-weight:bold;\n    }\n    .career{\n        padding:0 16px;\n        flex:1;\n        border-top:1px solid #eee;\n        border-bottom:1px solid #eee;\n    }\n    .about-me{\n        padding:0 16px;\n        flex:1;\n        height:16px;\n        border-bottom:1px solid #eee;\n    }\n    .project-image{\n        flex:4;\n        display:flex;\n        justify-content:center;\n\t\t\n    }\n    .bottom{\n        flex:2;\n        display: flex;\n        align-items: flex-end;\n        justify-content: space-between;\n    }\n    .language-skill{\n        display:flex;\n        align-items:flex-end;\n        height:6vw;\n        min-height:70px\n    }\n    .lang_icon{\n        height:3vw;\n        width:3vw;\n    }\n    .profile-image{\n        width:8vh;\n        height:8vh;\n        border-radius:50%;\n        border:8px solid rgb(52,152,219);\n        background-color:rgb(52,152,219);\n    }\n\n    @media (min-width: 0px) and (max-width: 640px) {\n        .box{\n            min-height: 360px;\n            min-width: 240px;\n        }\n\n    }\n    .topbtn{\n        border-radius:50%;\n        border:8px solid rgb(52,152,219);\n        background-color:rgb(52,152,219);\n        width:8vh;\n        height:8vh;\n        position:fixed;\n        bottom:24px;\n        right:32px;\n    }\n    .topbtn-image{\n        min-width:64px;\n        min-height:64px;\n    }\n\n</style>"]}]}